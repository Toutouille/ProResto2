<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tmary.proresto2" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Home"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".BookActivity"
            android:label="@string/title_book_activity"
            android:icon="@drawable/ic_calendar"/>
        <activity
            android:name=".OrderActivity"
            android:label="@string/title_order_activity"
            android:icon="@drawable/ic_order"/>
        <activity
            android:name=".MapActivity"
            android:label="@string/title_map_activity"
            android:icon="@drawable/ic_map"/>
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_profile_activity"
            android:icon="@drawable/ic_profile"/>

    </application>

</manifest>
